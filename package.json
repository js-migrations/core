{
  "name": "codedoor-app-client",
  "version": "1.14.0",
  "private": true,
  "homepage": "http://staging.codedoor.org",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "node scripts/build.js",
    "coverage": "npm run test:watch -- --coverage",
    "cypress:open": "cypress open",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "lint-test": "npm run lint && npm run test && npm run coverage",
    "start": "node scripts/start.js",
    "test": "CI=true npm run test:watch",
    "test:watch": "node scripts/test.js --env=jsdom",
    "test:watch:update": "npm run test:watch -- -u",
    "version-major": "npm version major && auto-changelog -p && git add CHANGELOG.md",
    "version-minor": "npm version minor && auto-changelog -p && git add CHANGELOG.md"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@okta/okta-react": "0.0.12",
    "@redux-offline/redux-offline": "2.3.2",
    "apisauce": "0.14.3",
    "autoprefixer": "8.1.0",
    "axios": "0.18.0",
    "bowser": "1.9.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "classnames": "2.2.5",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "flat": "4.0.0",
    "html-webpack-plugin": "2.30.1",
    "i18next": "10.5.1",
    "i18next-browser-languagedetector": "2.1.0",
    "moment": "2.20.1",
    "postcss-cssnext": "3.1.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.2",
    "react": "16.2.0",
    "react-async-script": "0.9.1",
    "react-dom": "16.2.0",
    "react-flexbox-grid": "2.0.0",
    "react-fontawesome": "1.6.1",
    "react-google-recaptcha": "0.9.9",
    "react-i18next": "7.5.0",
    "react-player": "1.3.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "next",
    "react-toolbox": "2.0.0-beta.12",
    "react-virtualized": "9.18.5",
    "redux": "3.7.2",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.7",
    "source-map-explorer": "1.5.0",
    "style-loader": "0.20.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "1.0.1",
    "victory": "0.25.6",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.2",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "author": "THM Software & Web Development",
  "devDependencies": {
    "auto-changelog": "1.4.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "22.4.1",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "3.1.1",
    "babel-runtime": "6.26.0",
    "chalk": "1.1.3",
    "cypress": "1.4.2",
    "dotenv": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.19.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "fetch-mock": "5.13.1",
    "fs-extra": "3.0.1",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "node-sass": "4.7.2",
    "object-assign": "4.1.1",
    "pre-push": "0.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "5.0.0",
    "redux-mock-store": "1.5.1",
    "sinon": "4.4.6"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false,
    "issueUrl": "https://github.com/CodeDoorHQ/codedoor-app-client/commit/{id}"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/containers/**/*.{js,jsx,mjs}",
      "src/components/**/*.{js,jsx,mjs}",
      "src/reducers/**/*.{js,jsx,mjs}",
      "!src/containers/examples/**/*.{js,jsx,mjs}",
      "!src/reducers/examples/**/*.{js,jsx,mjs}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 66.67,
        "functions": 82.14,
        "lines": 80.82,
        "statements": 80.49
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "pre-push": [
    "test",
    "lint"
  ]
}
